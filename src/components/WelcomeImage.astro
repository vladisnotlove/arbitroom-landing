<style is:global lang="scss">
    .welcome-image__atom {
        animation: atom-fly 6s infinite ease-in-out;
    }

    .welcome-image__electron1 {
        animation: electron-fly2 4s infinite ease-in-out;
    }

    .welcome-image__electron2 {
        animation: electron-fly1 2s infinite ease-in-out;
        animation-delay: -1.25s;
    }

    .welcome-image__electron3 {
        animation: electron-fly1 2s infinite ease-in-out;
    }

    @keyframes electron-fly1 {
        0% {
            transform: translateY(-10px);
        }

        50% {
            transform: translateY(10px);
        }

        100% {
            transform: translateY(-10px);
        }
    }

    @keyframes electron-fly2 {
        0% {
            transform: translateY(-15px);
        }

        50% {
            transform: translateY(15px);
        }

        100% {
            transform: translateY(-15px);
        }
    }

    @keyframes atom-fly {
        0% {
            transform: translateY(-7px);
        }

        50% {
            transform: translateY(7px);
        }

        100% {
            transform: translateY(-7px);
        }
    }
</style>
